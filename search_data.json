---
layout: null
sitemap:
  exclude: 'yes'
---
{
  {% for post in site.posts %}
    "{{ post.url | slugify }}": {
      "title": "{{ post.title | xml_escape }}",
      "url": "{{ post.url | xml_escape }}",
      "name": "{{ post.blogger.name }}",
      "slug": "{{ post.slug | xml_escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
}
